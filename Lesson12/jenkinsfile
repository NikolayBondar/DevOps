pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Apache2') {
            steps {
                        
                 
                sh"  id=$(ps -aux | grep '[a]pache2 -k start' | awk '$1=$1' | cut -d ' ' -f2 | head -n 1 | tr -d '/n')"
                           
            }
        }
    }
}
